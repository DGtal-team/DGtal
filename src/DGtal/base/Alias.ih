//////////////////////////////////////////////////////////////////////////////
#include <cstdlib>
//////////////////////////////////////////////////////////////////////////////

///////////////////////////////////////////////////////////////////////////////
// IMPLEMENTATION of inline methods.
///////////////////////////////////////////////////////////////////////////////

///////////////////////////////////////////////////////////////////////////////
// ----------------------- Standard services ------------------------------

//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::~Alias()
{}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::Alias()
  : myPtrT( 0 )
{}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T> &
DGtal::Alias<T>::operator=( Alias & other )
{ 
  myPtrT = other.myPtrT;
  return *this;
}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::Alias( T & t ) : myPtrT( &t )
{}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::Alias(const  Alias<T> & other ) : myPtrT( other.myPtrT )
{}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::Alias( T* pT ) : myPtrT( pT )
{}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::operator T*() const
{
  return myPtrT;
}
//-----------------------------------------------------------------------------
template <typename T>
inline
DGtal::Alias<T>::operator T&() const
{
  return *myPtrT;
}
