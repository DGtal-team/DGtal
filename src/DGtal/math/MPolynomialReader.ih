#include <cstdlib>
#include <iomanip>
//////////////////////////////////////////////////////////////////////////////


//////////////////////////////////////////////////////////////////////////////
// class MPolynomialReaderData<n, TRing>
//////////////////////////////////////////////////////////////////////////////



///////////////////////////////////////////////////////////////////////////////
// Interface - public :

/**
 * Writes/Displays the object on an output stream.
 * @param out the output stream where the object is written.
 */
template <int n, typename TRing, typename TIterator>
inline
void
DGtal::MPolynomialReader<n, TRing, TIterator>::
selfDisplay ( std::ostream & out ) const
{
  out << "[MPolynomialReader"; 
}

/**
 * Checks the validity/consistency of the object.
 * @return 'true' if the object is valid, 'false' otherwise.
 */
template <int n, typename TRing, typename TIterator>
inline
bool
DGtal::MPolynomialReader<n, TRing, TIterator>::isValid() const
{
    return true;
}



///////////////////////////////////////////////////////////////////////////////
// Implementation of inline functions                                        //

template <int n, typename TRing, typename TIterator>
inline
std::ostream&
DGtal::operator<< ( std::ostream & out, 
                    const MPolynomialReader<n, TRing, TIterator> & object )
{
  object.selfDisplay( out );
  return out;
}

