inline
bool 
DGtal::ArithmeticalDSS<TIterator,TInteger,connectivity>::extendOppositeEnd(const ConstIterator& it)
{

	return extend(it, myF, (Point(*myF) - Point(*it)), myUl, myUf, myLl, myLf); 

}

