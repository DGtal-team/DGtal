inline
bool 
DGtal::ArithmeticalDSS<TIterator,TInteger,connectivity>::extend()
{

  ConstIterator it(myL); ++it;
	return extend(it, myL, (Point(*it) - Point(*myL)), myUf, myUl, myLf, myLl); 

}

