//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uSetKCoord( PreCell & c, DGtal::Dimension k, Integer i )
{
  ASSERT( k < DIM );
  c.myCoordinates[ k ] = i;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sSetKCoord( SPreCell & c, DGtal::Dimension k, Integer i )
{
  ASSERT( k < DIM );
  c.myCoordinates[ k ] = i;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uSetCoord( PreCell & c, DGtal::Dimension k, Integer i )
{
  ASSERT( k < DIM );
  c.myCoordinates[ k ] = 2 * i + ( NumberTraits<Integer>::odd( c.myCoordinates[ k ] ) ? 1 : 0 );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sSetCoord( SPreCell & c, DGtal::Dimension k, Integer i )
{
  ASSERT( k < DIM );
  c.myCoordinates[ k ] = 2 * i + ( NumberTraits<Integer>::odd( c.myCoordinates[ k ] ) ? 1 : 0 );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uSetKCoords( PreCell & c, const Point & kp )
{
  c.myCoordinates = kp;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sSetKCoords( SPreCell & c, const Point & kp )
{
  c.myCoordinates = kp;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uSetCoords( PreCell & c, const Point & p )
{
  for ( DGtal::Dimension k = 0; k < DIM; ++k )
    uSetCoord( c, k, p[k] );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sSetCoords( SPreCell & c, const Point & p )
{
  for ( DGtal::Dimension k = 0; k < DIM; ++k )
    sSetCoord( c, k, p[k] );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sSetSign( SPreCell & c, Sign s )
{
  c.myPositive = ( s == POS );
}
//-----------------------------------------------------------------------------
// ------------------------- Cell topology services -----------------------
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
TInteger
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uTopology( const PreCell & p )
{
  Integer i = NumberTraits<Integer>::ZERO;
  Integer j = NumberTraits<Integer>::ONE;
  for ( DGtal::Dimension k = 0; k < DIM; ++k )
    {
      if ( NumberTraits<Integer>::odd( p.myCoordinates[ k ] ) )
        i |= j;
      j *= 2;
    }
  return i;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
TInteger
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sTopology( const SPreCell & p )
{
  Integer i = NumberTraits<Integer>::ZERO;
  Integer j = NumberTraits<Integer>::ONE;
  for ( DGtal::Dimension k = 0; k < DIM; ++k )
    {
      if ( NumberTraits<Integer>::odd( p.myCoordinates[ k ] ) )
        i |= j;
      j *= 2;
    }
  return i;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
DGtal::Dimension
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uDim( const PreCell & p )
{
  Integer i = NumberTraits<Integer>::ZERO;
  for ( DGtal::Dimension k = 0; k < DIM; ++k )
    if ( NumberTraits<Integer>::odd( p.myCoordinates[ k ] ) )
      ++i;
  return i;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
DGtal::Dimension
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sDim( const SPreCell & p )
{
  Integer i = NumberTraits<Integer>::ZERO;
  for ( DGtal::Dimension k = 0; k < DIM; ++k )
    if ( NumberTraits<Integer>::odd( p.myCoordinates[ k ] ) )
      ++i;
  return i;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
bool
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uIsSurfel( const PreCell & b )
{
  return uDim( b ) == ( DIM - 1 );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
bool
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sIsSurfel( const SPreCell & b )
{
 return sDim( b ) == ( DIM - 1 );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
bool
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uIsOpen( const PreCell & p, DGtal::Dimension k )
{
  return NumberTraits<Integer>::odd( p.myCoordinates[ k ] );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
bool
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sIsOpen( const SPreCell & p, DGtal::Dimension k )
{
  return NumberTraits<Integer>::odd( p.myCoordinates[ k ] );
}

