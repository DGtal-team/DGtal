//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
uAddFaces( PreCells& faces, const PreCell& c, Dimension axis )
{
  const DGtal::Dimension dim_of_c = uDim( c );
  if ( axis >= dim_of_c ) return;

  DirIterator q = uDirs( c );
  for ( Dimension i = 0; i < axis; ++i ) ++q;

  const Integer x = uKCoord( c, *q );

  PreCell f1 = uIncident( c, *q, false );
  PreCell f2 = uIncident( c, *q, true );

  faces.push_back( f1 );
  faces.push_back( f2 );

  uAddFaces( faces, f1, axis );
  uAddFaces( faces, f2, axis );

  uAddFaces( faces, c, axis+1 );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
void
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
uAddCoFaces( PreCells& cofaces, const PreCell& c, Dimension axis )
{
  const DGtal::Dimension dim_of_c = uDim( c );
  if ( axis >= dimension - dim_of_c ) return;

  DirIterator q = uOrthDirs( c );
  for ( Dimension i = 0; i < axis; ++i ) ++q;

  const Integer x = uKCoord( c, *q );

  PreCell f1 = uIncident( c, *q, false );
  PreCell f2 = uIncident( c, *q, true );

  cofaces.push_back( f1 );
  cofaces.push_back( f2 );

  uAddCoFaces( cofaces, f1, axis );
  uAddCoFaces( cofaces, f2, axis );

  uAddCoFaces( cofaces, c, axis+1 );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::PreCells
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
uFaces( const PreCell & c )
{
  PreCells N;
  uAddFaces( N, c, 0 );
  return N;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::PreCells
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
uCoFaces( const PreCell & c )
{
  PreCells N;
  uAddCoFaces( N, c, 0 );
  return N;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
bool
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
sDirect( const SPreCell & p, DGtal::Dimension k )
{
  ASSERT( k < dim );

  bool sign = p.myPositive;
  for ( DGtal::Dimension i = 0; i <= k; ++i )
    if ( sIsOpen( p, i ) )
      sign = ! sign;
  return sign;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
sDirectIncident( SPreCell p, DGtal::Dimension k )
{
  ASSERT( k < dim );

  bool sign = p.myPositive;
  for ( DGtal::Dimension i = 0; i <= k; ++i )
    if ( sIsOpen( p, i ) )
      sign = ! sign;

  bool up = sign;
  p.myPositive = POS;

  if ( up )  ++p.myCoordinates[ k ];
  else  --p.myCoordinates[ k ];

  return p;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
sIndirectIncident( SPreCell p, DGtal::Dimension k )
{
  ASSERT( k < dim );

  bool sign = p.myPositive;
  for ( DGtal::Dimension i = 0; i <= k; ++i )
    if ( sIsOpen( p, i ) )
      sign = ! sign;

  bool up = ! sign;
  p.myPositive = NEG;

  if ( up ) ++p.myCoordinates[ k ];
  else --p.myCoordinates[ k ];

  return p;
}




