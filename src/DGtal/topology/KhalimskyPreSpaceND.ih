// ----------------------- Incidence services --------------------------
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::PreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
uIncident( PreCell c, DGtal::Dimension k, bool up )
{
  ASSERT( k < dim );

  if ( up ) ++c.myCoordinates[ k ];
  else      --c.myCoordinates[ k ];

  return c;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
sIncident( SPreCell c, DGtal::Dimension k, bool up )
{
  ASSERT( k < dim );

  bool sign = up ? c.myPositive : ! c.myPositive;
  for ( DGtal::Dimension i = 0; i <= k; ++i )
    if ( sIsOpen( c, i ) )
      sign = ! sign;
  c.myPositive = sign;

  if ( up ) ++c.myCoordinates[ k ];
  else      --c.myCoordinates[ k ];

  return c;
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger >::PreCells
DGtal::KhalimskyPreSpaceND< dim, TInteger >::
uLowerIncident( const PreCell & c )
{
  PreCells N;
  for ( auto q = uDirs( c ); q != 0; ++q )
    {
      const DGtal::Dimension k = *q;
      N.push_back( uIncident( c, k, false ) );
      N.push_back( uIncident( c, k, true ) );
    }
  return N;
}
