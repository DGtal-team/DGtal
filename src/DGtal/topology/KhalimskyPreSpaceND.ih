template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::PreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uPreCell( const Point & kp )
{
  return { kp };
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::PreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uPreCell( Point p, const PreCell & c )
{
  for ( DGtal::Dimension i = 0; i < dimension; ++i )
    {
      p[ i ] += p[ i ] + ( NumberTraits<Integer>::odd( c.myCoordinates[ i ] ) ? 1 : 0 );
    }
  return uPreCell( p );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sPreCell( const Point & kp, Sign sign )
{
  return { kp, sign == POS };
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sPreCell( Point p, const SPreCell & c )
{
  for ( DGtal::Dimension i = 0; i < DIM; ++i )
    p[ i ] += p[ i ] + ( NumberTraits<Integer>::odd( c.myCoordinates[ i ] ) ? 1 : 0 );
  return sPreCell( p, c.myPositive );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::PreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uPreSpel( Point p )
{
  for ( DGtal::Dimension i = 0; i < DIM; ++i )
    p[ i ] += p[ i ] + 1;
  return uPreCell( p );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sPreSpel( Point p, Sign sign )
{
  for ( DGtal::Dimension i = 0; i < DIM; ++i )
    p[ i ] += p[ i ] + 1;
  return sPreCell( p, sign );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::PreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
uPrePointel( Point p )
{
  for ( DGtal::Dimension i = 0; i < DIM; ++i )
    p[ i ] += p[ i ];
  return uPreCell( p );
}
//-----------------------------------------------------------------------------
template < DGtal::Dimension dim, typename TInteger>
inline
typename DGtal::KhalimskyPreSpaceND< dim, TInteger>::SPreCell
DGtal::KhalimskyPreSpaceND< dim, TInteger>::
sPrePointel( Point p, Sign sign )
{
  for ( DGtal::Dimension i = 0; i < DIM; ++i )
    p[ i ] += p[ i ];
  return sPreCell( p, sign );
}
