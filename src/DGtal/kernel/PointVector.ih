template<DGtal::Dimension dim, typename TEuclideanRing, typename TContainer>
inline
typename DGtal::PointVector<dim, double, std::array<double,dim> >
DGtal::PointVector<dim, TEuclideanRing, TContainer>::getNormalized() const
{
  PointVector<dim,double,std::array<double,dim> > normalized =(*this);
  normalized /= normalized.norm();
  return normalized;
}
