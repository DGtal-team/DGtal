inline
DGtal::PointVector<dim,TEuclideanRing, TContainer>
DGtal::PointVector<dim,TEuclideanRing, TContainer>::diagonal( Component val )
{
  Self p;
  for ( DGtal::Dimension i = 0; i < dim; ++i )
    p.myArray[ i ] = val;
  return p;
}
